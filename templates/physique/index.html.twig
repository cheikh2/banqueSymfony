<div class="col-md-6 offset-md-3 mt-5">
	<div class="card">
		<h4 class="card-header text-center">BANQUE DU PEUPLE</h4>
		<div class="card-body">
			<div class="form-row">
				<div class="form-group col-md-4 offset-md-2">
					{{ form_label(form.prenom) }}
					{{ form_widget(form.prenom) }}
				</div>


				<div class="form-group col-md-4">
					{{ form_label(form.nom) }}
					{{ form_widget(form.nom) }}
				</div>
			</div>

			<div class="form-row">
				<div class="form-group col-md-4 offset-md-2">
					{{ form_label(form.adress) }}
					{{ form_widget(form.adress) }}
				</div>

				<div class="form-group col-md-4">
					{{ form_label(form.email) }}
					{{ form_widget(form.email) }}
				</div>
			</div>

			<div class="form-row">
				<div class="form-group col-md-4 offset-md-2">
					{{ form_label(form.telephone) }}
					{{ form_widget(form.telephone) }}
				</div>

				<div class="form-group col-md-4">
					{{ form_label(form.sexe) }}
					{{ form_widget(form.sexe) }}
				</div>
			</div>

			<div class="form-row">
				<div class="form-group col-md-4 offset-md-2">
					{{ form_label(form.profession) }}
					{{ form_widget(form.profession) }}
				</div>

				<div class="form-group col-md-4">
					{{ form_label(form.cni) }}
					{{ form_widget(form.cni) }}
				</div>
			</div>

			<div class="form-row">
				<div class="form-group col-md-4 offset-md-2">
					{{ form_label(form.salaire) }}
					{{ form_widget(form.salaire) }}
				</div>

				{{ form_label(form.moral) }}
				<div class="form-control col-md-4 mt-4">
					{{ form_widget(form.moral) }}
				</div>
			</div>
		</div>
	

